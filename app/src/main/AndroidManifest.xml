                android:name="android.support.PARENT_ACTIVITY"
                android:value="acbcmedia.com.Control_Page.ActivityOne" />
        </activity>
        <activity
            android:name=".Schedule.FriEvening"
            android:label="@string/title_activity_fri_evening"
