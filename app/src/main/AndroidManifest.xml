            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="acbcmedia.com.Control_Page.ActivityOne" />
        </activity>
        <activity
            android:name=".Schedule.SunDayService"
            android:label="@string/title_activity_sun_day_service"
