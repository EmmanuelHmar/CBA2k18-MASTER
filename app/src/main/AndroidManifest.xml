<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acbcmedia.com.Control_Page">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cbana_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_cbana_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityOne" />
        <activity android:name=".ActivityTwo" />
        <activity android:name=".ActivityThree" />
        <activity android:name=".ActivityFour" />
        <activity
            android:name=".Schedule.SatDayService"
            android:label="@string/title_activity_sat_day_service"
            android:parentActivityName=".ActivityOne"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="acbcmedia.com.Control_Page.ActivityOne" />
        </activity>
        <activity
            android:name=".Schedule.SatLunchDinner"
