<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="acbcmedia.com.Control_Page">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cbana_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_cbana_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="acbcmedia.com.Control_Page.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="acbcmedia.com.Control_Page.ActivityOne"></activity>
        <activity android:name="acbcmedia.com.Control_Page.ActivityTwo"></activity>
        <activity android:name="acbcmedia.com.Control_Page.ActivityThree"></activity>
        <activity android:name="acbcmedia.com.Control_Page.ActivityFour"></activity>
    </application>

</manifest>